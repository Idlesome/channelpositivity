{
  "name": "channelpositivity",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:all": "npm run gen && npm run build",
    "build": "next build && next export && next-image-export-optimizer",
    "build:serve:prod": "npm run build && npm run serve:prod",
    "serve:prod": "npx serve -l 3001 ./out",
    "start": "next dev",
    "lint": "next lint",
    "gen": "npm run gen:generate && npm run gen:format && npm run gen:validate",
    "gen:generate": "ts-node --project ./gen/tsconfig.json ./gen/gen.ts",
    "gen:format": "prettier ./data/articles/markdown/index.ts --write",
    "gen:validate": "tsc ./types.d.ts ./data/articles/markdown/index.ts --noEmit"
  },
  "dependencies": {
    "marked": "^4.0.18",
    "next": "^13.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.14",
    "@tailwindcss/typography": "^0.5.9",
    "@types/marked": "^4.0.3",
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.1.4",
    "file-loader": "^6.2.0",
    "json5": "^2.2.3",
    "next-image-export-optimizer": "^1.1.2",
    "notion-to-md": "^3.1.1",
    "postcss": "^8.4.21",
    "prettier": "^3.2.5",
    "raw-loader": "^4.0.2",
    "serve": "^14.2.1",
    "sharp": "0.31.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "4.7.4"
  }
}
